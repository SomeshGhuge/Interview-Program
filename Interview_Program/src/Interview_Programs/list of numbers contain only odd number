package practice;

import java.util.List;

public class list{
	 
	   public static boolean isOddNumbers(List<Integer> listofNumbers){
	       return listofNumbers.stream().allMatch(x-> (x%2) !=0);
	   }
	   public static void main(String[]args){
	    List<Integer> list = List.of(3,9);
	    
	    if(isOddNumbers(list)){
	     System.out.println("This  is  odd numbers list");
	    }else{
	      System.out.println("This is not a odd number list it containn even numbers");
	    }
	    
	   }
	 }
